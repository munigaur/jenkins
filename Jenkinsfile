    agent any

    stages {
        stage ('One') {
            steps {
            echo "I AM STAGE ONE STEP"
            }
            }

        stage ('Two') {
            steps {
             echo "I AM STAGE TWO STEP"           
            }
            }

        stage ('Three') {
            steps {
             echo "I AM STAGE THREE STEP"          
            }
            }

        }
}
